<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="capslock.kiddy_register.main.WriteController">
   <children>
      <Label text="以下の表は登録されている情報です.「登録情報をファイルに書き出す」ボタンを押すと情報をファイルに書き込みます" />
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="590.0">
        <columns>
          <TableColumn fx:id="keyCol" maxWidth="80.0" text="項目" />
          <TableColumn fx:id="valueCol" minWidth="8.0" prefWidth="511.0" text="データ" />
        </columns>
      </TableView>
      <Button fx:id="writeButton" mnemonicParsing="false" onAction="#onWritePushed" prefHeight="45.0" prefWidth="260.0" text="登録情報をファイルに書き出す">
         <font>
            <Font size="18.0" />
         </font></Button>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
   </padding>
</VBox>
