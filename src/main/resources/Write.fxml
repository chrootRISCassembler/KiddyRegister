<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="capslock.kiddy_register.main.WriteController">
   <children>
      <Label text="以下の表は登録されている情報です.「登録情報を確定する」ボタンを押すと情報をファイルに書き込みます" />
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="keyCol" prefWidth="75.0" text="項目" />
          <TableColumn fx:id="valueCol" prefWidth="75.0" text="データ" />
        </columns>
      </TableView>
      <Button fx:id="writeButton" mnemonicParsing="false" onAction="#onWritePushed" text="登録情報を確定する" />
   </children>
</VBox>
